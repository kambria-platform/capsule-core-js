{
  "name": "capsule-core-js",
  "version": "1.0.3",
  "description": "ðŸš€ CapsuleJS build a friendly interface of several Ethereum wallets and that might help developers can instantly power their Dapps.",
  "main": "index.js",
  "scripts": {
    "release": "npm run build && npm publish",
    "build": "npm run clean && npm run build-js",
    "build-js": "babel lib -d dist && babel main.js -o index.js",
    "clean": "rm -rf index.js && rm -rf dist && rm -rf ./node_modules/capsule-core-js",
    "fake-module": "mkdir -p ./node_modules/capsule-core-js && cp -R ./index.js ./node_modules/capsule-core-js && cp -R ./dist ./node_modules/capsule-core-js",
    "start": "npm run build && npm run fake-module && npm run serve",
    "serve": "HTTPS=true PORT=3003 REACT_APP_ENV=development react-scripts start",
    "test": "npm run build && mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kambria-platform/capsule-core-js.git"
  },
  "keywords": [
    "kambria",
    "capsule-wallet",
    "ethereum"
  ],
  "author": "Tu Phan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kambria-platform/capsule-core-js/issues"
  },
  "homepage": "https://github.com/kambria-platform/capsule-core-js#readme",
  "dependencies": {
    "@ledgerhq/hw-app-eth": "^4.62.0",
    "@ledgerhq/hw-transport-u2f": "^4.62.0",
    "@ledgerhq/hw-transport-webusb": "^4.62.0",
    "@myetherwallet/mewconnect-web-client": "^1.0.14",
    "bip39": "^3.0.2",
    "crypto-js": "^3.1.9-1",
    "ethereumjs-tx": "^1.3.7",
    "ethereumjs-util": "^6.1.0",
    "ethereumjs-wallet": "^0.6.3",
    "hdkey": "^1.1.1",
    "keythereum": "^1.0.4",
    "trezor-connect": "^7.0.3",
    "web3": "^0.20.7",
    "web3-provider-engine": "^15.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
